[ENV]
LOGDIR           = /opt/informix/logs
INFORMIXDIR      = /opt/informix/12.10.FC9DE
INFORMIXSERVER   = ol_informix1210
ONCONFIG         = onconfig.ol_informix1210
INFORMIXSQLHOSTS = /opt/informix/12.10.FC9DE/etc/sqlhosts.ol_informix1210
DBTEMP           = /tmp
DBCENTURY        = C
DBDATE           = y4md


[EMAIL]
MAILTO   = user1@example.com
MAILCC   = user2@example.com,manager@example.com
MAILFROM = mail.sender@example.com
# These values are only required if you must log into your email server
SMTPHOST = 
SMTPUSER = 
SMTPPASS = 

[CSVFILE]
# Day of week to send/clear csvfile: Sun = 0, Mon = 1, etc
CLEARDAY = 0
# Email the csv file?
MAILCSV  = no
# Should the file be cleared?
CLEARCSV = yes

[DBSPACES]
datadbs1 = 85
rootdbs  = 35
